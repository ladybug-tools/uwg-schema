      "UWG": {
        "title": "UWG",
        "description": "Urban Weather Generator (UWG) class.",
        "type": "object",
        "properties": {
          "bldheight": {
            "title": "Bldheight",
            "description": "Average urban building height in meters.",
            "minimum": 0,
            "type": "number",
            "format": "double"
          },
          "blddensity": {
            "title": "Blddensity",
            "description": "Value between 0 and 1 for building footprint density as fraction of urban area.",
            "minimum": 0,
            "maximum": 1,
            "type": "number",
            "format": "double"
          },
          "vertohor": {
            "title": "Vertohor",
            "description": "Value for vertical-to-horizontal urban area ratio. The vertical-to-horizontal urban area ratio is calculated by dividing the urban facade area by total urban area.",
            "minimum": 0,
            "type": "number",
            "format": "double"
          },
          "grasscover": {
            "title": "Grasscover",
            "description": "Number for fraction of urban ground covered in grass only.",
            "minimum": 0,
            "maximum": 1,
            "type": "number",
            "format": "double"
          },
          "treecover": {
            "title": "Treecover",
            "description": "Number for fraction of urban ground covered in trees.",
            "minimum": 0,
            "maximum": 1,
            "type": "number",
            "format": "double"
          },
          "zone": {
            "title": "Zone",
            "description": "Text representing an ASHRAE climate zone. This value is used to specify climate zone-specific construction, and HVAC parameters for the DOE reference building types. This will not effect the simulation if only custom reference buildings are used.  Choose from the following: \"1A\", \"1B\", \"2A\", \"2B\", \"3A\", \"3B-CA\", \"3B\", \"3C\", \"4A\", \"4B\", \"4C\", \"5A\", \"5B\", \"5C\", \"6A\", \"6B\", \"7\", \"8\".",
            "type": "string"
          },
          "h_mix": {
            "title": "H Mix",
            "description": "Value between 0 and 1 for fraction of HVAC waste heat released to street canyon. It is assumed the rest of building HVAC waste heat is released from the roof.",
            "minimum": 0,
            "maximum": 1,
            "type": "number",
            "format": "double"
          },
          "type": {
            "title": "Type",
            "default": "UWG",
            "pattern": "^UWG$",
            "type": "string",
            "readOnly": true
          },
          "version": {
            "title": "Version",
            "description": "Text string for the current version of the schema.",
            "default": "0.2.11",
            "pattern": "([0-9]+)\\.([0-9]+)\\.([0-9]+)",
            "type": "string"
          },
          "month": {
            "title": "Month",
            "description": "Number (1-12) representing simulation start month.",
            "default": 1,
            "minimum": 0,
            "maximum": 12,
            "type": "integer",
            "format": "int32"
          },
          "day": {
            "title": "Day",
            "description": "Number (1-31) representing simulation start day.",
            "default": 1,
            "minimum": 1,
            "maximum": 31,
            "type": "integer",
            "format": "int32"
          },
          "nday": {
            "title": "Nday",
            "description": "Number of days to simulate.",
            "default": 31,
            "minimum": 0,
            "type": "integer",
            "format": "int32"
          },
          "dtsim": {
            "title": "Dtsim",
            "description": "Simulation time step in seconds.",
            "default": 300,
            "minimum": 0,
            "type": "integer",
            "format": "int32"
          },
          "dtweather": {
            "title": "Dtweather",
            "description": "Number for weather data time-step in seconds.",
            "default": 3600,
            "minimum": 0,
            "type": "integer",
            "format": "int32"
          },
